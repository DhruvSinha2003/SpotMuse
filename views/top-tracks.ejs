<body>
    <h1>Top Tracks</h1>
    <% if (topTracks.length > 0) { %>
      <ul>
        <% topTracks.forEach(track => { %>
          <li><%= track.name %> by <%= track.artists[0].name %></li>
        <% }); %>
      </ul>
    <% } else { %>
      <p>No top tracks available.</p>
    <% } %>
    <a href="/">Go back</a>
  </body>
